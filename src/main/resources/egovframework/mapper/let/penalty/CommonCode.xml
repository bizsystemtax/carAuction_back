<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CommonCodeDAO">

	<resultMap id="CommonCodeList" type="egovframework.penalty.CommonCodeVO">
		<result property="cmmCd" column="CMM_CD"/>
		<result property="cmmCdNm" column="CMM_CD_NM"/>
	</resultMap>
	
		<resultMap id="selectCommonCode" type="egovframework.penalty.CommonCodeVO">
		<result property="cmmCd" column="CMM_CD"/>
	</resultMap>

	<select id="selectCommonCode" parameterType="egovframework.penalty.CommonCodeVO" resultMap="selectCommonCode">
		SELECT CMM_CD
		     , CMM_CD_NM
		  FROM TB_CMM_CD_LDGR
		 WHERE USE_YN = 'Y'
	</select>
	
</mapper>